generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
}

model Manga {
  id  Int @id @default(autoincrement())
  title String
  artist_id Int
  group_id Int
  parodies_id Int
  file_location String
  tumbnail_location String
  category_id Int
  source String?
  remarks String?
  filename String
}